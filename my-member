(defun my-member (%atm %lst)
	(let ()
		(defvar *tf* nil)
		(defvar *sz* (length %lst))
		(defvar *ret* '())
	) 
	(do 
		(
			(=i 0 (+ =i 1))
		)
		( (>= =i *sz*) *ret*) 

		(cond (
				(= *tf* t
					(setq =x (list (nth =i %lst)))
					(append =x *ret*)
				)
				(= (nth =i %lst) %atm
				 (setq *tf* t)
				 (setq =x (list (nth =i %lst)))
				 (append =x *ret*)
				)
			  )
		)
	)
)
	

