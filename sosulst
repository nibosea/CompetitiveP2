(defun sosulst (%num)
 (let (=lst '()))
 (setq =lst (makelst 2 %num))
 ;2~Nまでのループ 
 (do ((=i 0 (1+ =i))
	  (=len (length =lst) (setq =len (length =lst)))
	  )
  ;終了条件式 終了値式
  ((eq =i =len) =lst)
  ;n-th i の倍数の物を取り除く
  (do (
	   (=k (+(nth =i =lst)(nth =i =lst)) (+ =k (nth =i =lst)))
	  )
  		( (> =k %num) =k )
	 (setq =lst (remove =k =lst))
   )
  )
 )
